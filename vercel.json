{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  hheaders": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline'; object-src 'none'; base-uri 'self'"
        }
      ]
    }
  ]
}
